{
  "expo": {
    "name": "estudo-sqlite-expo",
    "slug": "estudo-sqlite-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "felipesobral",
    "icon": "./assets/images/icon.png",
    "scheme": "com.felipe.estudosqlite",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
